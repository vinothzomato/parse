{
    "name":"zomato/parse",
    "type":"library",
    "description":"A PHP Security Scanner for Zomato",
    "keywords":["security","scanner","static"],
    "homepage":"https://github.com/vinothzomato/parse",
    "license":"MIT",
    "authors":[
        {
            "name":"Vinoth Kumar",
            "email":"vinoth.kumar@zomato.com",
            "homepage":"http://www.zomato.com"
        }
    ],
    "require":{
        "php": ">=5.4",
        "nikic/php-parser": "^1.0|^2.0",
        "symfony/console": "~2.5|~3.1",
        "symfony/event-dispatcher": "~2.4|~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "codeclimate/php-test-reporter": "dev-master",
        "mockery/mockery": "0.9.*",
        "akamon/mockery-callable-mock": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Psecio\\Parse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psecio\\Parse\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/zomato-parse",
        "bin/parse"
    ]
}
